{
  "nvim vim.api snippet": {
    "prefix": "va",
    "body": "vim.api.$1"
  },
  "nvim vim.fn snippet": {
    "prefix": "vf",
    "body": "vim.fn.$1"
  },
  "vim print vim inspect": {
    "prefix": "vip",
    "body": "vim.print(vim.inspect($1))"
  },
  "vim print": {
    "prefix": "vp",
    "body": "vim.print($1)"
  }
}
