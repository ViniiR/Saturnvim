{
  "let%lwt": {
    "prefix": "letlwt",
    "body": ["let%lwt"]
  },
  "try%lwt": {
    "prefix": "trylwt",
    "body": ["try%lwt", "\t$1", "with _ -> ()"]
  },
  "(* comment *)": {
    "prefix": "co",
    "body": ["(* $1 *)"]
  },
  "printg": {
    "prefix": "pr",
    "body": ["Printf.printf \"$1\""]
  },
  "then...else": {
    "prefix": "then",
    "body": ["then", "\t$1", "else"]
  },
  "with _ -> ...": {
    "prefix": "with",
    "body": ["with", "\t| $1"]
  },
  "array": {
    "prefix": "[|",
    "body": ["[| $1 |]"]
  }
}
